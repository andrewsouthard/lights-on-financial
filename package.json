{
  "name": "LightsOnFinancial",
  "productName": "Lights On Financial",
  "version": "1.0.0",
  "description": "Lights On Financial Desktop App",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start --enable-logging",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "clean": "rm -rf out/ yarn-error.log *.xlsx"
  },
  "keywords": [],
  "author": "Andrew",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": ["squirrel"],
        "darwin": ["dmg"],
        "linux": ["deb"]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "LightsOnFinancial"
      },
      "electronInstallerDMG": {
        "icon": "icon.png"
      },
      "electronInstallerDebian": {
        "icon": "icon.png",
        "categories": ["Utility"],
        "homepage": "https://lightsonfinancial.com"
      },
      "electronInstallerRedhat": {
        "license": "Copyright Lights On Financial",
        "requires": ["lsb", "libappindictor"],
        "compressionLevel": 9,
        "categories": ["Utility"]
      },
      "github_repository": {
        "owner": "andrewsouthard",
        "name": "lights-on-financial",
        "draft": "true"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "lofdesktop"
      }
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "bootstrap": "^4.1.3",
    "electron": "^3.0.6",
    "electron-compile": "^6.4.3",
    "electron-devtools-installer": "^2.2.4",
    "electron-is": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.3.1",
    "lodash-core": "^0.8.1",
    "popper.js": "^1.14.4",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-navigation-prompt": "^1.8.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "sql.js": "^0.5.0",
    "util.promisify": "^1.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "3.0.6",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1"
  }
}
